<view class="container" style="padding:0;">
  <block wx:if="{{markers.length>0}}">
    <map latitude="{{center.latitude}}" longitude="{{center.longitude}}" scale="12" markers="{{markers}}" style="width:100%;height:100vh;"></map>
  </block>
  <block wx:else>
    <view class="empty">No recent events with location.</view>
  </block>
</view>
